<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#D05BE4"
    tools:context=".fragmentos.SegundoFragment">

    <!-- TODO: Update blank fragment layout -->
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Segundo fragment"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="35sp" />

    <EditText
        android:id="@+id/etUrl"
        android:layout_width="215sp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="49sp"
        android:layout_marginTop="60sp"
        android:ems="10"
        android:hint="Ingresa la url"
        android:textColor="#040000" />

    <WebView
        android:id="@+id/wvNavigator"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="110sp" />

    <Button
        android:id="@+id/btnSearch"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="265sp"
        android:layout_marginTop="60sp"
        android:text="Search" />

</FrameLayout>